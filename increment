import numpy as np
import time

start = time.time()
x = 0
elements = 100000000
s = 0

for x in range(0,elements,1):
    s += np.random.rand(1, elements)

end = time.time()
duration = end - start
print(s + "\n")
print(f"Duration: {duration} seconds")

# Duration: 426.43460297584534 seconds
# with AMD Athlon Silver 3050U 2.30Ghz
